            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <version>3.3.0</version>
                <configuration>
                    <archive>
                        <manifestFile>
                            src/main/resources/META-INF/MANIFEST.MF
                        </manifestFile>
                        <manifest>
                            <mainClass>sar.innovate..</mainClass>
                            <addClasspath>true</addClasspath>
                            <classpathPrefix>-lib/</classpathPrefix> <!-- add project name before -lib as convension -->
                        </manifest>
                        <manifestEntries>
                            <Built-By>sar</Built-By>
                        </manifestEntries>
                    </archive>
                </configuration>
            </plugin>
            
